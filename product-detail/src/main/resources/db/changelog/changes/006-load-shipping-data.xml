<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                        http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.23.xsd">

    <changeSet id="006-load-shipping-data" author="system">
        <loadData 
            file="db/changelog/data/shipping.csv"
            tableName="shipping"
            separator=","
            quotchar="&quot;">
            <column name="id" type="NUMERIC"/>
            <column name="id_product_detail" type="NUMERIC"/>
            <column name="free_shipping" type="BOOLEAN"/>
            <column name="logistic_type" type="STRING"/>
            <column name="mode" type="STRING"/>
        </loadData>
    </changeSet>

</databaseChangeLog>